                <script>
                    function DisplayToast(type,title,msg){
                       document.getElementById('title').value=title
                       document.getElementById('message').value=msg
                       document.getElementById(type).checked=true
                       document.getElementById('showtoast').click()
                    }
                    const socket = io("/")
                    socket.on('geoFencingBreak',function(data){
                        DisplayToast("toastDanger","Geo Fencing Alert",'Driver Name - '+data.pilotName+',Vehicle No - '+data.vehicleNo+'   <br><a href="/users/batteryActivity?batteryId='+data.batteryId+'"><button type="button" class="btn btn-link text-white" id="cleartoasts">Track Battery</button></a>')
                        
                    })
                    window.onload=SetLink()
                    function SetLink(){
                        currentLink=(window.location.pathname).split('users/')[1]
                        ArrayOfLink=[[''],['driver','driverprofile'],['e-vehicle']]
                        let i=0;
                        while(i < ArrayOfLink.length){
                            let j=0;
                            while(j < ArrayOfLink[i].length){
                                if(ArrayOfLink[i][j]==currentLink){
                                    document.getElementsByName("activateLink")[i].className="mm-active"
                                }
                                j++;
                            }   
                            i++;
                        }
                    }
            </script>
              